	keywords = {Formation flying, Solar radiation pressure, Linear controls, Perturbations, Dynamics},
	note = {Satellite Constellations and Formation Flying},
	number = {11},
	pages = {3396-3408},
	title = {Spacecraft formation and orbit control using differential attitude-dependent solar radiation pressure},
	url = {https://www.sciencedirect.com/science/article/pii/S0273117720302209},
	volume = {67},
	year = {2021},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0273117720302209},
	bdsk-url-2 = {https://doi.org/10.1016/j.asr.2020.03.047}}

@article{Burnett:2022aa,
	abstract = {This paper explores the problem of analytically approximating the orbital state for a subset of orbits in a rotating potential with oblateness and ellipticity perturbations. This is done by isolating approximate differential equations for the orbit radius and other elements. The conservation of the Jacobi integral is used to make the problem solvable to first order in the perturbations. The solutions are characterized as small deviations from an unperturbed circular orbit. The approximations are developed for near-circular orbits with initial mean motion n0 around a body with rotation rate c. The approximations are shown to be valid for values of angular rate ratio Gamma = c/n0 > 1, with accuracy decreasing as Gamma approaches 1, and singularities at and near Gamma = 1. Extensions of the methodology to eccentric orbits are discussed, with commentary on the challenges of obtaining generally valid solutions for both near-circular and eccentric orbits.},
	author = {Burnett, Ethan R. and Schaub, Hanspeter},
	date = {2022/01/20},
	date-added = {2024-05-30 19:55:16 +0200},
	date-modified = {2024-05-30 19:56:19 +0200},
	doi = {10.1007/s10569-022-10061-z},
