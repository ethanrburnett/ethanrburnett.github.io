	title = {Study of highly perturbed spacecraft formation dynamics via approximation},
	url = {https://www.sciencedirect.com/science/article/pii/S0273117720301393},
	volume = {67},
	year = {2021},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0273117720301393},
