	issn = {0273-1177},
	journal = {Advances in Space Research},
	keywords = {Formation flying, Astrodynamics, Asteroids, Perturbations, Dynamics, Linear systems},
	note = {Satellite Constellations and Formation Flying},
	number = {11},
